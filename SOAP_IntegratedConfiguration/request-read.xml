        <!-- <bas:ReceiverPartyID>YOUR_RECEIVER_PARTY</bas:ReceiverPartyID> -->
        <!-- <bas:ReceiverComponentID>YOUR_RECEIVER_COMPONENT</bas:ReceiverComponentID> -->
